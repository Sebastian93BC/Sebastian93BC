[
  {
    "$match": {
      "processName": "{{$json.processName}}",
      "$expr": {
        "$eq": [
          "$processDate", {
            "$toDate": "{{$json.processDate}}"
          }
        ]
      }
    }
  }, {
    "$project": {
      "_id": 0, 
      "proceso": "$processName", 
      "fechaProceso": "$processDate", 
      "status": 1
    }
  }
]