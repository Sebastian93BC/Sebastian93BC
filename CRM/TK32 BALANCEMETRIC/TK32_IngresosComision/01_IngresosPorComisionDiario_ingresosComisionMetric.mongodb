        {
          $match: {
            $expr: {
              $eq: [
                "$$todayDate",
                "$fecha_odate",
              ],
            },
          },
        },
        {
          $project: {
            nucta: 1,
            sal_act: {
              $multiply: ["$sal_act", -1],
            },
            sal_ant: {
              $multiply: ["$sal_ant", -1],
            },
            sal_net_act: {
              $multiply: ["$sal_net_act", -1],
            },
          },
        },
