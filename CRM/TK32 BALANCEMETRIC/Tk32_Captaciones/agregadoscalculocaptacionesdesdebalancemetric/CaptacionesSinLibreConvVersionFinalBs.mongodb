[
  {
    "$addFields": {
      "captacionesBs.sinLibreConv.actual": {
        "$subtract": [
          "$captacionesBs.total.actual", "$captacionesBs.libreConv.actual"
        ]
      }, 
      "captacionesBs.sinLibreConv.anterior": {
        "$subtract": [
          "$captacionesBs.total.anterior", "$captacionesBs.libreConv.anterior"
        ]
      }, 
      "captacionesBs.sinLibreConv.netoActual": {
        "$subtract": [
          "$captacionesBs.total.netoActual", "$captacionesBs.libreConv.netoActual"
        ]
      }
    }
  }, {
    "$project": {
      "_id": 0, 
      "captaciones": 0, 
      "cobranzaMora": 0, 
      "cobranzaMoraUVC": 0, 
      "liquidaciones": 0, 
      "liquidacionesUVC": 0, 
      "cobranzaMoraBs": 0, 
      "cobranzaMoraUVCBs": 0, 
      "liquidacionesBs": 0, 
      "liquidacionesUVCBs": 0, 
      "carteraCredito": 0, 
      "carteraCreditoBs": 0, 
      "ingresosComisiones": 0, 
      "ingresosComisionesBs": 0
    }
  }, {
    "$facet": {
      "corteMes": [
        {
          "$match": {
            "$expr": {
              "$eq": [
                "$fecha_valor", "$fechaProceso"
              ]
            }
          }
        }, {
          "$addFields": {
            "captacionesBs.sinLibreConv.mensual": "$captacionesBs.sinLibreConv.actual"
          }
        }, {
          "$project": {
            "captacionesBs.sinLibreConv.mensual": 1, 
            "fechaProceso": 1
          }
        }
      ], 
      "data": []
    }
  }, {
    "$unwind": {
      "path": "$data"
    }
  }, {
    "$addFields": {
      "data.captacionesBs.sinLibreConv.corteMes": "$corteMes", 
      "corteMes": "$$REMOVE"
    }
  }, {
    "$addFields": {
      "data.captacionesBs.sinLibreConv.mensual": {
        "$arrayElemAt": [
          {
            "$filter": {
              "input": "$data.captacionesBs.sinLibreConv.corteMes", 
              "as": "fecha", 
              "cond": {
                "$eq": [
                  "$$fecha.fechaProceso", "$data.fechaProceso"
                ]
              }
            }
          }, 0
        ]
      }, 
      "data.captacionesBs.sinLibreConv.captacionesDic": {
        "$arrayElemAt": [
          {
            "$filter": {
              "input": "$data.captacionesBs.sinLibreConv.corteMes", 
              "as": "fecha", 
              "cond": {
                "$and": [
                  {
                    "$eq": [
                      {
                        "$month": "$$fecha.fechaProceso"
                      }, 12
                    ]
                  }, {
                    "$ne": [
                      {
                        "$year": "$data.fechaProces"
                      }, {
                        "$year": "$$fecha.fechaProceso"
                      }
                    ]
                  }
                ]
              }
            }
          }, 0
        ]
      }
    }
  }, {
    "$addFields": {
      "data.captacionesBs.sinLibreConv.mensual": "$data.captacionesBs.sinLibreConv.mensual.captacionesBs.sinLibreConv.mensual", 
      "data.captacionesBs.sinLibreConv.corteMes": "$$REMOVE", 
      "data.captacionesBs.sinLibreConv.captacionesDic": "$data.captacionesBs.sinLibreConv.captacionesDic.captacionesBs.sinLibreConv.mensual", 
      "data.captacionesBs.sinLibreConv.varMensual": {
        "$subtract": [
          "$data.captacionesBs.sinLibreConv.actual", "$data.captacionesBs.sinLibreConv.anterior"
        ]
      }
    }
  }, {
    "$addFields": {
      "data.captacionesBs.sinLibreConv.varAnual": {
        "$cond": [
          {
            "$eq": [
              {
                "$year": "$data.fechaProceso"
              }, {
                "$year": "$$NOW"
              }
            ]
          }, {
            "$subtract": [
              "$data.captacionesBs.sinLibreConv.actual", "$data.captacionesBs.sinLibreConv.captacionesDic"
            ]
          }, 0
        ]
      }, 
      "data.captacionesBs.sinLibreConv.creAnual": {
        "$cond": [
          {
            "$eq": [
              {
                "$year": "$data.fechaProceso"
              }, {
                "$year": "$$NOW"
              }
            ]
          }, {
            "$round": [
              {
                "$divide": [
                  {
                    "$multiply": [
                      {
                        "$subtract": [
                          "$data.captacionesBs.sinLibreConv.actual", "$data.captacionesBs.sinLibreConv.captacionesDic"
                        ]
                      }, 100
                    ]
                  }, "$data.captacionesBs.sinLibreConv.captacionesDic"
                ]
              }, 4
            ]
          }, 0
        ]
      }
    }
  }, {
    "$facet": {
      "data": [
        {
          "$match": {
            "data.captacionesBs.sinLibreConv.actual": {
              "$ne": null
            }
          }
        }
      ], 
      "dataNotNull": [
        {
          "$match": {
            "data.captacionesBs.sinLibreConv.actual": {
              "$ne": null
            }
          }
        }, {
          "$group": {
            "_id": {
              "fechaProceso": "$data.fechaProceso", 
              "fechaValor": "$data.fecha_valor"
            }, 
            "maxFechaValor": {
              "$max": "$data.fecha_valor"
            }, 
            "maxMensual": {
              "$max": "$data.captacionesBs.sinLibreConv.actual"
            }
          }
        }, {
          "$sort": {
            "_id.fechaValor": -1
          }
        }
      ]
    }
  }, {
    "$addFields": {
      "ultimoActual": {
        "$slice": [
          "$dataNotNull", 1
        ]
      }, 
      "dataNotNull": "$$REMOVE"
    }
  }, {
    "$unwind": {
      "path": "$ultimoActual"
    }
  }, {
    "$unwind": {
      "path": "$data"
    }
  }, {
    "$addFields": {
      "data.data.captacionesBs.sinLibreConv.mensual": {
        "$cond": [
          {
            "$eq": [
              "$data.data.fechaProceso", "$ultimoActual._id.fechaProceso"
            ]
          }, "$ultimoActual.maxMensual", "$data.data.captacionesBs.sinLibreConv.mensual"
        ]
      }, 
      "ultimoActual": "$$REMOVE"
    }
  }, {
    "$replaceRoot": {
      "newRoot": "$data.data"
    }
  }, {
    "$merge": {
      "into": "Balancemetric", 
      "on": "fecha_valor"
    }
  }
]