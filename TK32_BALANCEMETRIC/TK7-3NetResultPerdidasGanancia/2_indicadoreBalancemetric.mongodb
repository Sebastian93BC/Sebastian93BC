[
  {
    "$match": {
      "$expr": {
        "$eq": [
          {
            "$subtract": [
              {
                "$dateTrunc": {
                  "date": "$$NOW", 
                  "unit": "day"
                }
              }, {
                "$multiply": [
                  {
                    "$toInt": "{{$json.offSet}}"
                  }, 86400000
                ]
              }
            ]
          }, "$fecha_valor"
        ]
      }
    }
  }, {
    "$lookup": {
      "from": "DailyGroupPerdidasGanaciasNR", 
      "localField": "previusMonthDate", 
      "foreignField": "fecha_valor", 
      "as": "date_previusMonthDate"
    }
  }, {
    "$lookup": {
      "from": "DailyGroupPerdidasGanaciasNR", 
      "localField": "previusYearDate", 
      "foreignField": "fecha_valor", 
      "as": "data_previusYearDate"
    }
  }, {
    "$lookup": {
      "from": "DailyGroupPerdidasGanaciasNR", 
      "localField": "yesterdayDate", 
      "foreignField": "fecha_valor", 
      "as": "data_yesterdayDate"
    }
  }, {
    "$lookup": {
      "from": "Balancemetric", 
      "localField": "previusYearDate", 
      "foreignField": "fecha_valor", 
      "as": "Balancemetric_data_previusYearDate"
    }
  }, {
    "$lookup": {
      "from": "Balancemetric", 
      "localField": "previusMonthDate", 
      "foreignField": "fecha_valor", 
      "as": "Balancemetric_data_previusMonthDate"
    }
  }, {
    "$lookup": {
      "from": "Balancemetric", 
      "let": {
        "fecha_valor": "$fecha_valor"
      }, 
