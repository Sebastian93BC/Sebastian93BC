        {
          "$match": {
            "$expr": {
              "$eq": [
                "$fecha_valor", "$$fecha_valor"
              ]
            }
          }
        }, {
          "$project": {
            "netResult": 1, 
            "netResultBs": 1
          }
        }
      ], 
      "as": "Balancemetric"
    }
  }, {
    "$replaceRoot": {
      "newRoot": {
        "$mergeObjects": [
          "$$ROOT", {
            "$first": "$Balancemetric"
          }
        ]
      }
    }
