        }
      }, 
      "sno_serial_nego": {
        "$trim": {
          "input": "$sno_serial_nego"
        }
      }, 
      "sno_regla_negoc": {
        "$trim": {
          "input": "$sno_regla_negoc"
        }
      }, 
      "sno_id_perfil": {
        "$trim": {
          "input": "$sno_id_perfil"
        }
      }, 
      "sno_tipo_registro": {
        "$trim": {
          "input": "$sno_tipo_registro"
        }
      }, 
      "sno_interfaz": {
        "$trim": {
          "input": "$sno_interfaz"
        }
      }
    }
  }, {
    "$addFields": {
      "sno_num_negoc": {
        "$convert": {
          "input": "$sno_num_negoc", 
          "to": "int", 
          "onError": 0, 
          "onNull": 0
        }
      }, 
      "sno_id_debito": {
        "$convert": {
          "input": "$sno_id_debito", 
          "to": "int", 
          "onError": 0, 
          "onNull": 0
        }
      }, 
      "fechaProceso": {
        "$subtract": [
          {
            "$dateFromParts": {
              "year": {
                "$year": "$sno_fec_valor"
              }, 
              "month": {
                "$sum": [
                  {
                    "$month": "$sno_fec_valor"
                  }, 1
                ]
              }
            }
          }, 86400000
        ]
      }
    }
  }, {
    "$merge": {
      "into": "sidis_ordenante", 
      "on": [
        "sno_rif_empresa", "sno_id_debito", "_id"
      ], 
      "whenNotMatched": "insert"
    }
  }
]