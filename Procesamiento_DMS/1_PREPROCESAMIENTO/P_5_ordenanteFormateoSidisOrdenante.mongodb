      }, 
      "sno_num_negoc": {
        "$convert": {
          "input": "$sno_num_negoc", 
          "to": "int", 
          "onError": 0, 
          "onNull": 0
        }
      }, 
      "sno_id_debito": {
        "$convert": {
          "input": "$sno_id_debito", 
          "to": "int", 
          "onError": 0, 
          "onNull": 0
        }
      }, 
      "sno_mto_debito": {
        "$convert": {
          "input": "$sno_mto_debito", 
          "to": "decimal", 
          "onError": 0, 
          "onNull": 0
        }
      }, 
      "sno_mto_actuali": {
        "$convert": {
