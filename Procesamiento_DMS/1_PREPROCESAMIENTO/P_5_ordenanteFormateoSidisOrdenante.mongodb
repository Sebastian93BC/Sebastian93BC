        }
      }, 
      "sno_mto_cobcomi": {
        "$convert": {
          "input": "$sno_mto_cobcomi", 
          "to": "decimal", 
          "onError": 0, 
          "onNull": 0
        }
      }, 
      "sno_fec_valor": {
        "$convert": {
          "input": "$sno_fec_valor", 
          "to": "date", 
          "onError": "$sno_fec_valor", 
          "onNull": "$$REMOVE"
        }
      }, 
      "sno_fec_inineg": {
        "$convert": {
          "input": "$sno_fec_inineg", 
          "to": "date", 
          "onError": "$sno_fec_inineg", 
          "onNull": "$$REMOVE"
        }
      }, 
      "sno_fec_recarch": {
        "$convert": {
          "input": "$sno_fec_recarch", 
          "to": "date", 
          "onError": "$sno_fec_recarch", 
          "onNull": "$$REMOVE"
        }
      }, 
      "sno_num_cuenta": {
        "$trim": {
          "input": "$sno_num_cuenta"
        }
      }, 
      "sno_id_reflote": {
        "$trim": {
