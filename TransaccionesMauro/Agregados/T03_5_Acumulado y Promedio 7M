[
    {
        "$addFields": {
            "fechaSuperior": {
                "$dateFromParts": {
                    "year": {
                        "$year": {
                            "$toDate": "@fechaProceso"
                        }
                    },
                    "month": {
                        "$month": {
                            "$toDate": "@fechaProceso"
                        }
                    }
                }
            }
        }
    },
    {
        "$addFields": {
            "fechaProcesoAct": {
                "$dateFromParts": {
                    "year": {
                        "$year": "$fechaProceso"
                    },
                    "month": {
                        "$month": "$fechaProceso"
                    }
                }
            },
            "fechaInferior": {
                "$dateSubtract": {
                    "startDate": {
                        "$dateFromParts": {
                            "year": {
                                "$year": "$fechaSuperior"
                            },
                            "month": {
                                "$month": "$fechaSuperior"
                            }
                        }
                    },
                    "unit": "month",
                    "amount": 6
                }
            }
        }
    },
    {
        "$match": {
            "$and": [
                {
                    "$expr": {
                        "$gte": [
                            "$fechaProcesoAct",
                            "$fechaInferior"
                        ]
                    }
                },
                {
                    "$expr": {
                        "$lte": [
                            "$fechaProcesoAct",
                            "$fechaSuperior"
                        ]
                    }
                },
                {
                    "rifCedula": {
                        "$nin": [
                            "",
                            "00000000"
                        ]
                    }
                }
            ]
        }
    },
    {
        "$group": {
            "_id": {
                "rifCedula": "$rifCedula"
            },
            "rifCedula": {
                "$first": "$rifCedula"
            },
            "fechaProceso": {
                "$max": "$fechaProceso"
            },
            "totalMesBolivares7m": {
                "$sum": "$totalMesBolivares"
            },
            "totalMesDolares7m": {
                "$sum": "$totalMesDolares"
            },
            "totalMesEuros7m": {
                "$sum": "$totalMesEuros"
            }
        }
    },
    {
        "$addFields": {
            "totalPromBolivares7m": {
                "$round": [
                    {
                        "$divide": [
                            "$totalMesBolivares7m",
                            7
                        ]
                    },
                    4
                ]
            },
            "totalPromDolares7m": {
                "$round": [
                    {
                        "$divide": [
                            "$totalMesDolares7m",
                            7
                        ]
                    },
                    4
                ]
            },
            "totalPromEuros7m": {
                "$round": [
                    {
                        "$divide": [
                            "$totalMesEuros7m",
                            7
                        ]
                    },
                    4
                ]
            },
            "rifCedula": "$rifCedula",
            "_id": "$$REMOVE",
            "acumuladoBolivares7m": {
                "$sum": "$totalMesBolivares7m"
            },
            "acumuladoEuros7m": {
                "$sum": "$totalMesEuros7m"
            },
            "acumuladoDolares7m": {
                "$sum": "$totalMesDolares7m"
            },
            "totalMesBolivares7m": "$$REMOVE",
            "totalMesDolares7m": "$$REMOVE",
            "totalMesEuros7m": "$$REMOVE"
        }
    },
    {
        "$merge": {
            "into": "sidis_ingresoComisiones",
            "on": [
                "fechaProceso",
                "rifCedula"
            ],
            "whenNotMatched": "discard"
        }
    }
]